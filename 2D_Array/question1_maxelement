public class MaxElement {
    public static void main(String[] args) {
        int[][] arr = {
            {11, 25, 36},
            {47, 52, 63},
            {74, 85, 96}
        };

        int max = arr[0][0];
        for (int i = 0; i < arr.length; i++) {
            for (int j = 0; j < arr[i].length; j++) {
                if (arr[i][j] > max) {
                    max = arr[i][j];
                }
            }
        }
        System.out.println("Largest element = " + max);
    }
}
